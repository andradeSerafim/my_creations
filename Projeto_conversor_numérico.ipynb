def parte_decimal_em_binario(depois_do_ponto):
  resultado = []
  while len(resultado) <= 16:
    depois_do_ponto *= 2
    digito = depois_do_ponto // 1
    resultado.append(int(digito))
    if digito != 0:
      depois_do_ponto -= 1
  return resultado


